@{
    ViewBag.Title = "Index";
    string frameSrc = "";// ViewData["menu"];
}

<!DOCTYPE html>
<html>
<head>
	<meta charset="UTF-8">
	<title>Full Layout - jQuery EasyUI Demo</title>
	<link rel="stylesheet" type="text/css" href="../../../Content/jquery-easyui/themes/default/easyui.css">
    <link href="../../../Content/jquery-easyui/themes/icon.css" rel="stylesheet" type="text/css" />
    <link href="../../../Content/jquery-easyui/demo.css" rel="stylesheet" type="text/css" />
    <script src="../../../Content/jquery-easyui/jquery.min.js" type="text/javascript"></script>
    <script src="../../../Content/jquery-easyui/jquery.easyui.min.js" type="text/javascript"></script>
</head>
<body class="easyui-layout">
	<div data-options="region:'north',border:false" style="height:60px;background:#B3DFDA;padding:10px">nav 清单</div>
	<div data-options="region:'west',split:true,title:'menu'" style="width:150px;padding:10px;">
    @Html.Action("Index", "TreeTest");
    </div>
	<div data-options="region:'east',split:true,collapsed:true,title:'Shoppinglist'" style="width:100px;padding:10px;">east region</div>
	<div data-options="region:'south',border:false" style="height:50px;background:#A9FACD;padding:10px;">south region</div>
	<div data-options="region:'center',title:''">
    <iframe scrolling="yes" frameborder="0"  src="@frameSrc" style="width:100%;height:100%;"></iframe>
    </div>
</body>
</html>